.sfPageWrapper .cards_grid, .sfPageWrapper .cards_line, .sfPageWrapper .hb_area { margin-bottom: 0px; min-height: 50px; }

.sfPageWrapper .cards_grid > .RadDockZone, .sfPageWrapper .cards_line > .RadDockZone, .sfPageWrapper .hb_area > .RadDockZone { display: flex; min-height: 50px; }

.sfPageWrapper .pp_images { margin-bottom: 0px; min-height: 50px; }

.sfPageWrapper .pp_images > .RadDockZone { display: flex; min-height: 50px; flex-wrap: wrap; }

.sfPageWrapper .pp_images > .RadDockZone .rdContent { display: flex; flex-wrap: wrap; justify-content: center; }

.sfPageWrapper .max_4_cards > .RadDockZone { display: flex; }

.sfPageWrapper .max_4_cards > .RadDockZone > .RadDock { flex: 1 0 24%; margin-right: 10px; }

.sfPageWrapper .max_4_cards > .RadDockZone > .RadDock .rdContent { display: flex; }

.sfPageWrapper .cards_grid > .RadDockZone { flex-wrap: wrap; }

.sfPageWrapper .cards_grid > .RadDockZone > .RadDock { flex: 0 0 24%; margin-right: 10px; }

.sfPageWrapper .resource_modern_area { display: block; }

.sfPageWrapper .resource_modern_area .RadDock { display: inline-block; width: 200px !important; }

.top_menu_cell:first-of-type .submenu:first-of-type li:last-child:not(has_undermenu):not(.undermenu_cell) { background-color: rgb(191, 197, 217); padding: 5px; }
